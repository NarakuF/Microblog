<div class="row">
  <div class="col-md-12 my-5">
     <h1>All Users</h1>
  </div>
  <%= for user <- @users do %>
    <div class="col-md-3 my-2">
      <div class="card">
        <div class="card-body">
          <h4 class="card-title">
            <%= link user.email, to: user_path(@conn, :show, user), class: "text-dark" %>
          </h4>
        </div>
      </div>
    </div>
  <% end %>
</div>
